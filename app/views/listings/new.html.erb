<%= form_for @listing do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %><br />
  <%= f.label :body, "Body" %>
  <%= f.text_area :body %><br />
  <%= f.label :seller_email, "Seller Email" %>
  <%= f.text_field :seller_email %><br />
  <%= f.label :zip, "Zip" %>
  <%= f.text_field :zip %><br />
  <% Tag.all.each do |tag| %>
    <div>
      <%= check_box_tag "listing[tag_ids][]", tag.id, @listing.tags.include?(tag.id) %>
      <%= label_tag :tag_ids, tag.name %>
    </div>
  <% end %>
  <%= f.submit %>
<% end %>